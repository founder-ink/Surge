.search-bar {
  width: 100%;
  height: 66rpx;
  display: flex;
  align-items: center;
  background: #f9f9f9;
  border-radius: 8px;
  border: 1px solid #e7e7e7;
}

.search-bar image {
  width: 32rpx;
  height: 32rpx;
  margin: 0 20rpx 0 30rpx;
}

.search-bar .search-input {
  flex: 1;
  font-size: 22rpx;
  color: #bfbfbf;
}