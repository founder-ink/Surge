/* Pages/setting/setting.wxss */

Page {
  background: #F7F7F7;
}
.container {
  padding: 10% 8%;
}

.setting-box {
  width: 100%;
  border-radius: 20rpx;
  background: #FEFFFE;
  margin-bottom: 50rpx;
}

.setting-box:last-child {
  color: #DD2B2D;
}

.about {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-bottom: 1px solid #F7F7F7;
}

.about:last-child {
  border: none;
}

.about-title {
  text-decoration: underline;
  font-size: 24rpx;
}
