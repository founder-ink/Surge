<!--miniprogram/custom-tab-bar/index.wxml-->
<cover-view class="tab-bar">
  <cover-view wx:for="{{list}}" wx:key="index" class="{{selected === index ? item.active : item.normal}}" data-path="{{item.pagePath}}" data-index="{{index}}" bindtap="switchTab">
    <cover-view class="iconfont {{item.iconfont}}" ></cover-view>
  </cover-view>
</cover-view>